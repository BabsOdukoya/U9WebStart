<DAT name="SFUNC" xml:space='preserve'>#define CREATE
;----------------------------------------------------------------
;
;----------------------------------------------------------------
operation create
public web
;----------------------------------------------------------------
params
	Struct pOptions : IN
endparams

variables
	String vId
endvariables

	;Extract the ID if it has been passed in
	vId = pOptions-&gt;"param"

	;If the ID is empty then we're invoking this DSP for entering
	;a new user, otherwise we're editting an existing user
	if(vId == "")
		;Configure the screen for entering a new record
		$fieldproperties(DELETE_BUTTON.BUTTONS, "style:display") = "style:display=none"
	else
		;Configure the screen for maintaining an existing record
	endif

	return 0

end ;create



#define SETUP
;----------------------------------------------------------------
; Run every time form is shown
;----------------------------------------------------------------
operation setup
;----------------------------------------------------------------
params
	Struct pOptions : IN
endparams

variables
	String vId
	Handle vService
	String vXml
endvariables

	;Extract the ID if it has been passed in
	vId = pOptions-&gt;"param"

	;If the ID is empty then we're invoking this DSP for entering
	;a new user. No need to go fetch any data.
	if(vId == "")
		;Just clear out any old data from the last time we used the DSP
		clear/e "APP_USERDSP"
		return 0
	endif

	newinstance "APP_USER_SVC", vService
	if($status &lt; 0)
		webmessage/error "Failed to create service (%%$status%%%)"
		return 0
	endif
	
	$status = vService-&gt;get(pOptions-&gt;"param", vXml)
	if($status &lt; 0)
		webmessage/error "Load failed (%%$status%%%)"
		return 0
	endif

	clear/e "APP_USERDSP"

	xmlload/incldefmap vXml, "DTD:&lt;DATA_DTD&gt;"
	if($status &lt; 0)
		webmessage/error "Xmlload failed (%%$status%%%)"
		return 0
	endif

	retrieve/reconnect "APP_USERDSP"
	if($status != 0)
		webmessage/error "retrieve/reconnect failed (%%$status%%%)"
		return 0
	endif

	return 0

end ;setup


