operation save
;----------------------------------------------------------------
public web

	call lpSave()
	webactivate $instancename.goBack()

	return 0

end ;save


;----------------------------------------------------------------
;
;----------------------------------------------------------------
operation delete
;----------------------------------------------------------------
public web

	remocc "APP_USERDSP", 1
	call lpSave()
	webactivate $instancename.goBack()
	
	return 0

end ;delete


