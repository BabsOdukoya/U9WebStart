<DAT name="UFTRIGGERS" xml:space='preserve'>;------------------------------------------
webtrigger onChange
;------------------------------------------

javascript
	var somethingTicked = false;

	if( this.getValue() == "1" ) {
		field  = this.getInstance().getEntity("BUTTONS.DUMMY").getOccurrence(0).getField("DELETE");
		field.setProperty("html:disabled", "false");
	}
	else {
		var occs = this.getInstance().getEntity("APP_USER.APP").getOccurrences();
		for (var i = 0; i &lt; occs.length; i++) {
			if( this.getValue() == "1" ) {
				somethingTicked = true;
			}
		}

		if(!somethingTicked) {
			field.setProperty("html:disabled", "true");
		}
	}
endjavascript

end ;onChange</DAT>
