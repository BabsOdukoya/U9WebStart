<DAT name="GENERAL" xml:space='preserve'>;---------------------------------------------
entry lpGetLayout
;---------------------------------------------
returns String

variables
	String vNavigationType
endvariables

	vNavigationType = $lowercase( $logical("NAVIGATION_TYPE") )

	selectcase vNavigationType
		case "top"
			return "app_users.html"
		case "dashboard"
			return "app_users.html"
		case "dashboard_2"
			return "app_users_dashboard_2.html"
		elsecase
			return "app_users.html"
	endselectcase

end ;lpGetLayout


