<DAT name="SFUNC" xml:space='preserve'>#define SETUP
;----------------------------------------------------------------
; TODO: Need to clear this when logging out otherwise it
;       doesn't get run for a new user. Maybe destroy all
;       instances when logging out?
;----------------------------------------------------------------
operation setup
;----------------------------------------------------------------
params
	Struct pOptions : IN
endparams

variables
	String vId
	Handle vService
	String vXml
endvariables

	newinstance "APP_USER_SVC", vService
	if($status &lt; 0)
		webmessage/error "Failed to create service (%%$status%%%)"
		return 0
	endif

	$status = vService-&gt;load(vXml)
	if($status &lt; 0)
		webmessage/error "Load failed (%%$status%%%)"
		return 0
	endif

	clear/e "APP_USERDSP"

	xmlload/incldefmap vXml, "DTD:&lt;DATA_DTD&gt;"
	if($status &lt; 0)
		webmessage/error "Xmlload failed (%%$status%%%)"
		return 0
	endif

	retrieve/reconnect "APP_USERDSP"
	if($status != 0)
		webmessage/error "retrieve/reconnect failed (%%$status%%%)"
		return 0
	endif

	return 0

end ;setup


#define REFRESH
;----------------------------------------------------------------
;
;----------------------------------------------------------------
operation refresh
;----------------------------------------------------------------
params
	Struct pOptions : IN
endparams

variables
	String vOptionsString
endvariables

	;If asked to refresh we're actually just going to do
	;the same as if we were setting up the screen normally
	structToJson vOptionsString, pOptions
	$instancehandle-&gt;appSetup(vOptionsString)

	return 0

end ;refresh


