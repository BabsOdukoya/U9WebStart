<DAT name="SFUNC" xml:space='preserve'>;----------------------------------------------------------------
; Confirm Button Callback
;----------------------------------------------------------------
operation confirmCallback
;----------------------------------------------------------------
public web
scope
	operation &lt;ALERT_DSP&gt;.showInfo
endscope

params
	Numeric pResult : IN
endparams

	selectcase pResult
		case 0
			;User pressed 'Yes' in the delete dialog
			;call lpDeleteAccount()
			;activate "&lt;MAIN_DSP&gt;".logOff()
			activate "&lt;ALERT_DSP&gt;".showInfo("Status", "You selected Yes")
			return 0
		case 1
			;User pressed 'No' in the delete dialog
			activate "&lt;ALERT_DSP&gt;".showInfo("Status", "You selected No")
			return 0
		elsecase
			;Unexpected result, bomb out
			return -1
	endselectcase

	return 0

end ;confirmCallback



;----------------------------------------------------------------
; More Button Callback
;----------------------------------------------------------------
operation moreCallback
;----------------------------------------------------------------
public web
scope
	operation &lt;ALERT_DSP&gt;.showInfo
endscope
params
	String pResult : IN
endparams

	if (pResult = 0)
		activate "&lt;ALERT_DSP&gt;".showInfo("Return Value", "0 - Close button")
	;else
	;	activate "&lt;ALERT_DSP&gt;".showInfo("Return Value", pResult)
	endif
	return 0

end ;moreCallback


;----------------------------------------------------------------
