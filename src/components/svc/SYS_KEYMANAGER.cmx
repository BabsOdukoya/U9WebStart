<DAT name="LISTING" xml:space='preserve'>Multi-implementation Technical Key Generator
Author: David Akerman
Usage: 
a) Configure .ASN logical KEYGENERATOR
	&lt;undefined&gt; - next sequence number from SYS_COUNTER table
	DBSEQ        - Database Sequence Number
           GUID           - Numeric conversion of $uuid

b) If using SYS_COUNTER table, create a new path and assign SYS_COUNTER.SYSTEM to that path in your .asn [FILES] section
This will prevent a commit to the sequence number update affecting your default path

b) Configure other logicals 

KEYGEN_PATH - commit path if using SYS_COUNTER mechanism (as per redirect above)
                            ** Note, should include $ prefix, e.g. $CTR
                         - or path to issue SQL command on if using DBSEQ mechanism

if using DBSEQ, configure additional logicals
	DBMS - ORA for Oracle (others not supported yet)
	SEQ_LOOKUP_CPT - Component which contains an operation to lookup the sequence name (see below)
	SEQ_LOOKUP_OP - Operation in above component to return a sequence name for a supplid table (see code for parameters)
	SEQ_PREFIX          - If lookup operation not supplied, prefix to add to table name to generate sequence name
	SEQ_SUFFIX         - as above, e.g. "_SEQ" default would be TABLE_SEQ

c) Add SYSTEM_LIBRARY:ERRF_KEY error handling code to your ID field error trigger
</DAT>
<DAT name="PERF">#DEF</DAT>
<DAT name="WINPROP">ATTACHED=</DAT>
<DAT name="SFUNC" xml:space='preserve'>; Important Note: see Component Comments for usage instructions
;--------------------------------------------------------------------------------
;	Constructor
;		$logical("KEYGENERATOR") determines mechanism
;		Currently supported:
;			&lt;undefined&gt; - Next Sequence number from SYS_COUNTER table
;							- requires KEYGEN_PATH logical
;			DBSEQ       - Database sequence number
;							- requires $DBMS_TYPE &amp; KEYGEN_PATH logicals
;			UUID        - GUID (string) with "-" removed
;--------------------------------------------------------------------------------
operation INIT
variables
	string vKeyGen
endvariables

	vKeyGen = $logical("KEYGENERATOR")
	if (vKeyGen != "")

		$METHOD$ = $concat("GetKey", vKeyGen)
		$KEYGEN_PATH$ = $logical("KEYGEN_PATH")
		if ($KEYGEN_PATH$ = "")
			$KEYGEN_PATH$ = "DEF"
		endif

		; Database Sequence Configuration
		if (vKeyGen = "DBSEQ")
			$DBMS_TYPE$ = $logical("DBMS")
			if ($DBMS_TYPE$ = "")
				$DBMS_TYPE$ = "ORA"
			endif
			if ($DBMS_TYPE$ = "ORA")
				$TEMPLATE_SQL$ = "SELECT &lt;seq_name&gt;.NEXTVAL FROM DUAL"
			endif
			$SEQ_LOOKUP_CPT$ = $logical("SEQ_LOOKUP_CPT")
			$SEQ_LOOKUP_OP$  = $logical("SEQ_LOOKUP_OP")
			if ($SEQ_LOOKUP_CPT$ = "")
				$SEQ_PREFIX$ = $logical("SEQ_PREFIX")
				$SEQ_SUFFIX$ = $logical("SEQ_SUFFIX")
				if ($SEQ_PREFIX$ = "" &amp; $SEQ_SUFFIX$ = "")
					$SEQ_SUFFIX$ = "_SEQ"
				endif
			endif
		endif
	
	else
		$METHOD$ = "GetKeyUUID"
	endif


end ; INIT


;--------------------------------------------------------------------------------
;	GetID - Generic operation to key a primary key ID
;	It will call a specific operation based on a logical (see INIT operation)
;	e.g. GetKey, GetKeyGUID, GetKeySequence
;--------------------------------------------------------------------------------
operation GetID
params
	$P_TABLE$       : IN 
	$P_VALUE$       : OUT
	$P_STATUS$      : OUT
	$P_MESSAGE$     : OUT
endparams

variables
	string V_COUNT
endvariables

if (!$P_TABLE$)
	$P_STATUS$ = -1
	$P_MESSAGE$ = "Table not specified"
	return(-1)
endif

activate $instancename."%%$METHOD$%%%"($P_TABLE$, $P_VALUE$, $P_STATUS$, $P_MESSAGE$)

end ; GetID


;--------------------------------------
;	Get key from DBMS table sequence
;--------------------------------------
operation GetKey
params
	$P_TABLE$       : IN 
	$P_VALUE$       : OUT
	$P_STATUS$      : OUT
	$P_MESSAGE$     : OUT
endparams
variables
	numeric vCount
endvariables

clear
&lt;COUNTERID&gt;.&lt;COUNTERENT&gt; = $P_TABLE$

call lpRetrieve
if ( $status = -11 )
	vCount = 1
	clear
	&lt;COUNTERID&gt;.&lt;COUNTERENT&gt; = $P_TABLE$
	call lpRetrieve
	while ($status = -11 &amp; vCount &lt; 110)
		vCount += 1
		retrieve
	endwhile
	if (!&lt;COUNTERVAL&gt;.&lt;COUNTERENT&gt;)
		$P_STATUS$ = -4
		$P_MESSAGE$ = "Counter %%$P_TABLE$%%% locked"
		return(-1)
	endif
elseif ($status &lt; 0)
	call lpCreateCounter
	if ($status &lt; 0)
		$P_STATUS$ = -2
		$P_MESSAGE$ = "Error creating Counter: "
		return(-1)
	endif
endif

&lt;COUNTERVAL&gt;.&lt;COUNTERENT&gt; += 1
#ifdefined &lt;COUNTERMAX&gt;
if ((&lt;COUNTERMAX&gt;.&lt;COUNTERENT&gt;) &amp; (&lt;COUNTERVAL&gt;.&lt;COUNTERENT&gt; &gt; &lt;COUNTERMAX&gt;.&lt;COUNTERENT&gt;))
	if (&lt;COUNTERMIN&gt;.&lt;COUNTERENT&gt;)
		&lt;COUNTERVAL&gt;.&lt;COUNTERENT&gt; = &lt;COUNTERMIN&gt;.&lt;COUNTERENT&gt;
	else
		&lt;COUNTERVAL&gt;.&lt;COUNTERENT&gt; = 1
	endif
endif
#endif
call lpStore

if ($status &lt; 0)
	$P_STATUS$ = -3
	$P_MESSAGE$ = "Store error"
	return (-1)
endif

$P_VALUE$ = &lt;COUNTERVAL&gt;.&lt;COUNTERENT&gt;
$P_STATUS$ = 0
return(0)

end  ; GetKey


;-------------------------------------------------------------------------
;	Get key from GUID
;   returns a 32 character string, e.g. 21EC20203AEA1069A2DD08002B30309D
;-------------------------------------------------------------------------
operation GetKeyUUID
params
	$P_TABLE$       : IN 
	$P_VALUE$       : OUT
	$P_STATUS$      : OUT
	$P_MESSAGE$     : OUT
endparams

	$P_VALUE$ = $replace($uuid, 1, "-", "", -1)
	return 0

end ; GetKeyUUID


;-------------------------------------------------------------------------
;	Get key from Database Sequence Number
;-------------------------------------------------------------------------
operation GetKeyDBSeq
params
	$P_TABLE$       : IN 
	$P_VALUE$       : OUT
	$P_STATUS$      : OUT
	$P_MESSAGE$     : OUT
endparams

	; If different table, rebuild SQL to get sequence number
	if ($P_TABLE$ != $P_LAST_TABLE$)
		$P_LAST_TABLE$ = $P_TABLE$

		; Lookup or generate sequence name from table name
		if ($SEQ_LOOKUP_CPT$ != "" &amp; $SEQ_LOOKUP_OP$ != "")
			activate "%%$SEQ_LOOKUP_CPT$%%%"."%%$SEQ_LOOKUP_OP$%%%"($P_TABLE$, $SEQ_NAME$, $P_STATUS$, $P_MESSAGE$)
			if ($procerror &lt; 0)
				$P_STATUS$  = $procerror
				return -1
			endif
			if ($P_STATUS$ &lt; 0)
				return $P_STATUS$
			endif
		else
			$SEQ_NAME$ = $concat($SEQ_PREFIX$, $P_TABLE$, $SEQ_SUFFIX$)
		endif

		; e.g. "SELECT &lt;seq_name&gt;.NEXTVAL FROM DUAL"
		$SQL$ = $replace($TEMPLATE_SQL$, 1, "&lt;seq_name&gt;", $SEQ_NAME$, 1)
	endif

	; Run SQL to get value
	sql $SQL$, $KEYGEN_PATH$
	$P_VALUE$  = $result
	$P_STATUS$ = $status
	if ($P_STATUS$ &lt; 0)
		$P_MESSAGE$ = $procerrorcontext
	endif
	return $P_STATUS$

end ; getKeyDBSeq
