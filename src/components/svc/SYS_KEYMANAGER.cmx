<DAT name="GENERAL" xml:space='preserve'>;---------------------------------
; Retrieve
;---------------------------------
entry lpRetrieve

retrieve
if ($procerror = -3)
	close
	return(-3)
;	retrieve
endif
return $status

end ; lpRetrieve


;---------------------------------
; Create Counter
;---------------------------------
entry lpCreateCounter

if (!$P_TABLE$)
	return(-1)
endif

clear
&lt;COUNTERID&gt;.&lt;COUNTERENT&gt;  = $P_TABLE$
&lt;COUNTERVAL&gt;.&lt;COUNTERENT&gt; = 0

call lpStore
if ($status &lt; 0)
	$P_MESSAGE$ = "Store Error"
	return(-5)
endif

retrieve/e "&lt;COUNTERENT&gt;"
if ($status &lt; 0)
	$P_MESSAGE$ = "Retrieve error"
	return(-6)
endif

return (0)

end ; lpCreateCounter
   

;---------------------------------
; Store
;---------------------------------
entry lpStore

store
if ($status &lt;0)
	rollback
	return(-3)
else
	commit "%%$KEYGEN_PATH$%%%"
	if ($status &lt; 0)
		rollback
		return(-3)
	endif
endif

end ; lpStore

